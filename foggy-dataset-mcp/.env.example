# MCP Data Model Java - Environment Configuration

# Server Configuration
SERVER_PORT=7108

# OpenAI / Aliyun Bailian Configuration
# 阿里云百炼兼容 OpenAI 接口，只需配置 base-url 即可切换
OPENAI_API_KEY=sk-your-api-key
OPENAI_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
OPENAI_MODEL=qwen-plus

# External Services
DATASET_QUERY_URL=http://localhost:8080
CHART_RENDER_URL=http://localhost:3000
CHART_RENDER_TOKEN=default-render-token

# MCP Service Layer (m1, m2, both)
MCP_LAYER=both
MCP_M1_PORT=7108
MCP_M2_PORT=7109

# M2 Query Expert Configuration
M2_MAX_ITERATIONS=10
M2_MAX_CONSECUTIVE_ERRORS=3
M2_TIMEOUT_SECONDS=120

# Logging
LOG_LEVEL=DEBUG
